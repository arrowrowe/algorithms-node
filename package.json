  "bugs": {
    "url": "https://github.com/arrowrowe/algorithms-node/issues"
  },
  "homepage": "https://github.com/arrowrowe/algorithms-node",
  "devDependencies": {
    "chai": "3.4.1",
    "eslint": "1.10.3",
    "ghooks": "1.0.1",
    "istanbul": "0.4.1",
    "mocha": "2.3.4"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm test"
    }
